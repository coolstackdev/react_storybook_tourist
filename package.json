{
  "name": "instahop",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "10.19.0",
    "yarn": "1.22.4"
  },
  "scripts": {
    "go": "concurrently \"yarn run start\" \"yarn run storybook\"",
    "start": "yarn run dev",
    "env:dev": "cross-env NODE_ENV=development",
    "env:prod": "cross-env NODE_ENV=production",
    "test": "clear && jest",
    "test:debug": "node --inspect node_modules/.bin/jest --watch --runInBand",
    "coverage": "yarn test -- --coverage",
    "postcoverage": "opn coverage/lcov-report/index.html",
    "lint": "eslint src private webpack.config.js",
    "storybook": "start-storybook -p 9001 -c private/storybook",
    "dev": "yarn run env:dev webpack-dev-server --host 0.0.0.0",
    "build:clean": "rimraf \"dist/!(.git*|Procfile)**\"",
    "build:copy": "copyfiles -u 1 public/* public/**/* dist",
    "prebuild": "yarn run build:clean && yarn run build:copy",
    "build": "yarn run env:prod -- webpack",
    "sitemap": "node ./sitemap-generator.js"
  },
  "jest": {
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/private/jest/fileMock.js",
      "^components$": "<rootDir>/private/jest/componentsMock.js"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/private/jest/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
